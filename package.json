{
  "name": "dashblocks",
  "version": "0.5.10",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/slanatech/dashblocks"
  },
  "author": "https://github.com/sv2",
  "homepage": "https://github.com/slanatech/dashblocks",
  "bugs": {
    "url": "https://github.com/slanatech/dashblocks/issues",
    "email": "sv2@slana.tech"
  },
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build-lib": "vue-cli-service build --target lib --report --name dashblocks ./src/components/index.js",
    "build-wc": "vue-cli-service build --target wc --name dashblocks 'src/components/**/*.vue'",
    "build-wc-async": "vue-cli-service build --target wc-async --name dash-blocks 'src/components/**/*.vue'",
    "demo:build": "vue-cli-service build --dest demo",
    "demo:deploy": "yarn run demo:build && ./scripts/demodeploy.sh",
    "build": "yarn run build-lib",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "test": "yarn run test:unit",
    "prettier": "prettier --single-quote --write src/**/*.js src/**/*.vue",
    "release": "release-it",
    "release-ci": "release-it --ci"
  },
  "files": [
    "dist/*",
    "src/assets/*",
    "src/components/*",
    "public/*",
    "package.json",
    "README.md",
    "CONTRIBUTING.md",
    "LICENSE"
  ],
  "main": "dist/dashblocks.common.js",
  "dependencies": {
    "vue": "^2.6.10",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.0",
    "plotly.js-dist": "^1.47.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-e2e-cypress": "^3.6.0",
    "@vue/cli-plugin-eslint": "^3.6.0",
    "@vue/cli-plugin-unit-jest": "^3.6.3",
    "@vue/cli-service": "^3.6.0",
    "@vue/eslint-config-prettier": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "brace": "^0.11.1",
    "chart.js": "^2.8.0",
    "chartjs-plugin-labels": "^1.1.0",
    "d3": "^5.9.2",
    "datalib": "^1.9.2",
    "deepmerge": "^3.2.0",
    "dygraphs": "^2.1.0",
    "easy-pie-chart": "^2.1.7",
    "lint-staged": "^7.3.0",
    "loglevel": "^1.6.1",
    "moment": "^2.24.0",
    "node-sass": "^4.11.0",
    "ramda": "^0.26.1",
    "release-it": "^12.3.0",
    "sass-loader": "^7.1.0",
    "vue-sidebar-menu": "^3.7.0",
    "vue-template-compiler": "^2.6.10",
    "vuebars": "^0.1.3",
    "vuetify": "^1.5.12",
    "vuetrend": "^0.3.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "release-it": {
    "github": {
      "release": true
    },
    "hooks": {
      "before:init": [
        "yarn run build",
        "yarn test"
      ],
      "after:release": "echo Successfully released ${name} v${version} to ${repo.repository}."
    }
  }
}
